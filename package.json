{
  "name": "twodark-gh-pages",
  "version": "0.9.8",
  "description": "TwoDark Theme for Sublime Text GitHub Page.",
  "main": "./src/server.js",
  "scripts": {
    "preinstall": "chmod +x -R ./hooks/;ln ./hooks/* ./git/hooks/",
    "start": "node ./src/server.js",
    "test": "./node_modules/.bin/mocha",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha",
    "bundle": "./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack -w -d",
    "build": "./node_modules/.bin/webpack -p --config ./webpack.config.prod.js --optimize-minimize --optimize-dedupe",
    "docs": "./node_modules/.bin/jsdoc -c ./src/.jsdocrc --readme ./src/README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erremauro/TwoDark.git"
  },
  "keywords": [
    "theme",
    "sublime",
    "text",
    "atom",
    "onedark",
    "spacegray",
    "editor",
    "packagecontrol"
  ],
  "author": "Roberto Mauro <erremauro@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erremauro/TwoDark/issues"
  },
  "homepage": "https://github.com/erremauro/TwoDark#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-istanbul": "^0.8.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-enzyme": "^0.5.0",
    "color": "^0.11.3",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "ignore-styles": "^4.0.0",
    "iphone-inline-video": "^1.9.3",
    "istanbul": "^0.4.4",
    "jsdoc": "^3.4.0",
    "jsdom": "^9.3.0",
    "json-loader": "^0.5.4",
    "markdown-loader": "^0.1.7",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "https://github.com/gaearon/react-hot-loader.git",
    "sass-loader": "^3.2.2",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
